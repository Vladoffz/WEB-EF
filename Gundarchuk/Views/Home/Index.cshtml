@using Gundarchuk.Data
@model IEnumerable<Gundarchuk.Models.Player>
@{
    Layout = null;
}
<div>
    <h3>Распродажа книг</h3>
    <table>
        <tr class="header">
            <td><p>Название книги</p></td>
            <td><p>Автор</p></td>
            <td><p>Цена</p></td>
            <td></td>
        </tr>
        @foreach (var item in Gundarchuk.Models.Player)
        {
            <tr>
                <td><p>@item.Id</p></td>
                <td>
                    <p>@item.FirstName</p>
                </td>
                <td>
                    <p>@item.LastName</p>
                </td>
                <td>
                    <p>@item.Age</p>
                </td>
                <td><p>@item.FirstMatch</p></td>
                <td><p>@item.Salary</p></td>
            </tr>
        }
    </table>
</div>